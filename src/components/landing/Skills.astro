                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                </div>
            </div>
                    
            <div class="flex-1 justify-center mx-0 px-10 ">
                <h2 class="text-3xl text-center underline py-5 font-bold">Professional Skills</h2>
                <div class="grid grid-cols-1 md:grid-cols-2 gap-4 ">
                
                    <div class=" flex-col flex items-center justify-center ">
                        <h2 class="text-center">
                            Creativity
                        </h2>
                        <div class=" flex items-center justify-center  rounded-full h-28 w-28   border-red-600 border-2 m-4 ">
                            100%
                        </div>
                        
                    </div>
                    <div class=" flex-col flex items-center justify-center ">
                        <h2 class="text-center">
                           Commucation
                        </h2>
                        <div class=" flex rounded-full h-28 w-28 items-center justify-center  border-red-600 border-2 m-4 ">
                            100%
                        </div>
                        
                    </div>
                    <div class=" flex-col flex items-center justify-center   ">
                        <h2 class="text-center">
                            Problem solving
                        </h2>
                        <div class=" flex rounded-full h-28 w-28 items-center justify-center  border-red-600 border-2 m-4 ">
                            100%
                        </div>
                        
                    </div>
                    <div class=" flex-col flex items-center justify-center   ">
                        <h2 class="text-center">
                            Teamwork
                        </h2>
                        <div class=" flex rounded-full h-28 w-28 items-center justify-center  border-red-600 border-2 m-4 ">
                            100%
                        </div>
                        
                    </div>
                  </div>
            </div>
        </div>
    </div>

</section>