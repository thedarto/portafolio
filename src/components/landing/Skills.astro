---
import 'boxicons/css/boxicons.min.css';
---
<section id="skills" class=" flex flex-col h-auto pb-0 px-2 mx-14 ">
    <div>
        <h1 class="text-5xl font-bold text-center my-20">
            My <span class="text-cyan-400">Skills</span>
        </h1>
        <div class="flex md:mx-4 ">
            <div class="flex-1 ">
                <h2 class=" text-3xl text-center underline my-5 font-bold ">Technical Skils</h2>
            
                <div class=" flex flex-col my-0 mx-3 md:mx-6">
                    <div>
                        <i class='bx bxl-html5 text-5xl' ></i>
                        <div>
                            <span>HTML</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-full rounded-full h-2 my-5 "></span>100%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-css3 text-5xl' ></i>
                        <div>
                            <span>CSS</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-javascript text-5xl' ></i>
                        <div>
                            <span>Javascript</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-6/12 rounded-full h-2 my-5  "></span>50%
                        </div>
                        
                    </div>
                    <div>
                        <i class='bx bxl-python text-5xl' ></i>
                        <div>
                            <span>Python</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-c-plus-plus text-5xl' ></i>
                        <div>
                            <span>C#</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-java text-5xl' ></i>
                        <div>
                            <span>Java</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-angular text-5xl' ></i>
                        <div>
                            <span>Angular</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-react text-5xl' ></i>
                        <div>
                            <span>React</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bxl-spring-boot text-5xl' ></i>
                        <div>
                            <span>Spring</span>
                        </div>
                        <div class=" flex  mr-10  mb-0">
                            <span class=" bg-cyan-400 w-8/12 rounded-full h-2 my-5  "></span>70%
                        </div>
                    </div>
                    <div>
                        <i class='bx bx-git-merge text-5xl' ></i>
